    <actor name="actor2">
      <pose>3 -5 1.25 0 0 0</pose>
      <skin>
        <filename>walk-blue.dae</filename> <!--run.dae-->
        <scale>1.0</scale>
      </skin>
      <animation name="walking">
        <filename>walk-blue.dae</filename>
        <scale>1.000000</scale>
        <interpolate_x>true</interpolate_x>
      </animation>

      <plugin name="actor2_plugin" filename="libPedestrianSFMPlugin.so">
        <velocity>0.6</velocity>
        <radius>0.5</radius>
        <animation_factor>5.1</animation_factor>
        <people_distance>0.0</people_distance>
        <obstacle_weight>0.0</obstacle_weight>
        <ignore_obstacles>
          <model>cafe</model>
          <model>ground_plane</model>
        </ignore_obstacles>
        <trajectory>
          <cyclic>true</cyclic>
          <waypoint>3 -5 1.25</waypoint>
          <waypoint>3 1 1.25</waypoint>
        </trajectory>
      </plugin>
    </actor>




